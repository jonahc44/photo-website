{
    "compilerOptions": {
      "target": "ES2022",
      "module": "CommonJS",
      "strict": true,
      "esModuleInterop": true,
      "outDir": "./dist",
      "rootDir": "./src",
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true
    },
    "include": ["src/*"],
    "exclude": ["node_modules"]
  }
  